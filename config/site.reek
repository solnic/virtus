---
UncommunicativeParameterName:
  accept: []
  exclude: []
  enabled: true
  reject:
  - !ruby/regexp /^.$/
  - !ruby/regexp /[0-9]$/
  - !ruby/regexp /[A-Z]/
LargeClass:
  max_methods: 14
  exclude: []
  enabled: true
  max_instance_variables: 8
UncommunicativeMethodName:
  accept: []
  exclude: []
  enabled: true
  reject:
  - !ruby/regexp /^[a-z]$/
  - !ruby/regexp /[0-9]$/
  - !ruby/regexp /[A-Z]/
LongParameterList:
  max_params: 3
  exclude: []
  enabled: true
  overrides: {}
FeatureEnvy:
  exclude: [
    Virtus::Attribute::Boolean#value_coerced?,
    Virtus::ClassMethods#build_attribute,
    Virtus::Coercion::TimeCoercions#to_string,
    Virtus::Coercion::TimeCoercions#coerce_with_method,
    Virtus::TypeLookup#determine_type_from_primitive,
    Virtus::ValueObject::ClassMethods#attribute
  ]
  enabled: true
ClassVariable:
  exclude: []
  enabled: true
BooleanParameter:
  exclude: []
  enabled: true
IrresponsibleModule:
  exclude: []
  enabled: true
UncommunicativeModuleName:
  accept: []
  exclude: []
  enabled: true
  reject:
  - !ruby/regexp /^.$/
  - !ruby/regexp /[0-9]$/
NestedIterators:
  ignore_iterators: []
  exclude: []
  enabled: true
  max_allowed_nesting: 2
LongMethod:
  max_statements: 6
  exclude: []
  enabled: true
Duplication:
  allow_calls: []
  exclude: [
    Virtus::Attribute::DefaultValue#evaluate
  ]
  enabled: true
  max_calls: 1
UtilityFunction:
  max_helper_calls: 1
  exclude: [
    Virtus::Attribute::Boolean#value_coerced?,
    Virtus::ClassMethods#build_attribute
  ]
  enabled: true
Attribute:
  exclude: []
  enabled: false
UncommunicativeVariableName:
  accept: []
  exclude: []
  enabled: true
  reject:
  - !ruby/regexp /^.$/
  - !ruby/regexp /[0-9]$/
  - !ruby/regexp /[A-Z]/
SimulatedPolymorphism:
  exclude: []
  enabled: true
  max_ifs: 1
DataClump:
  exclude: []
  enabled: true
  max_copies: 2
  min_clump_size: 2
ControlCouple:
  exclude: [
    'Virtus::InstanceMethods#initialize'
  ]
  enabled: true
LongYieldList:
  max_params: 1
  exclude: []
  enabled: true
